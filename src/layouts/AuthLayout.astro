---
import BaseLayout from './BaseLayout.astro';

interface Props {
  title: string;
}

const { title } = Astro.props;
---

<BaseLayout title={title}>
  <main class="min-h-screen flex items-center justify-center p-4">
    <div class="w-full max-w-md">
      <slot />
    </div>
  </main>
</BaseLayout>